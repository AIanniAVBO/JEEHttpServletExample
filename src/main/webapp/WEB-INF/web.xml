<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>JEEHttpServletExample</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>HelloServlet</display-name>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>org.avbo.tpsit.servletexample.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SumServlet</display-name>
    <servlet-name>SumServlet</servlet-name>
    <servlet-class>org.avbo.tpsit.servletexample.operations.SumServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SumServlet</servlet-name>
    <url-pattern>/sum</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DiffServlet</display-name>
    <servlet-name>DiffServlet</servlet-name>
    <servlet-class>org.avbo.tpsit.servletexample.operations.DiffServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DiffServlet</servlet-name>
    <url-pattern>/diff</url-pattern>
  </servlet-mapping>
</web-app>